{
  "cli": {
    "version": ">= 3.14.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release",
        "simulator": true
      }
    },
    "production": {
      "android": {
        "releaseChannel": "default"
      },
      "ios": {
        "releaseChannel": "default"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
