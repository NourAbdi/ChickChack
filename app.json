{
  "expo": {
    "name": "ChickChack",
    "slug": "chickchack",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2662FD"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.ChickChack.ChickChack",
      "config": {
        "googleMapsApiKey": "AIzaSyBdL3GkLFaqTFFlOYfTG4eRl2UBQM3l2yw"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#2662FD"
      },
      "package": "com.ChickChack.ChickChack",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBdL3GkLFaqTFFlOYfTG4eRl2UBQM3l2yw"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "0ff3ac09-132d-49ed-9b94-7347ad37d330"
      }
    },
    "owner": "chick",
    "plugins": [
      "expo-localization",
      "expo-location",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  }
}
